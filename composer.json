{
    "name": "yedincisenol/sms",
    "description": "Send sms with Php",
    "type": "library",
    "license": "MIT",
    "version": "1.4.6",
    "authors": [
        {
            "name": "Ibrahim S. Orencik",
            "email": "o@yedincisenol.com"
        }
    ],
    "require": {
        "php": ">=7.0",
        "guzzlehttp/guzzle": "^6.2",
        "ext-simplexml": "*",
        "ext-json": "*"
    },
    "autoload": {
        "psr-4" : {
            "yedincisenol\\Sms\\" : "src"
        }
